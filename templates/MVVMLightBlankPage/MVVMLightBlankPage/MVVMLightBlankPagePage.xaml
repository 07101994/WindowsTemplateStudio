<Page
    x:Class="Page_NS.MVVMLightBlankPage.MVVMLightBlankPagePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    DataContext="{Binding MVVMLightBlankPageViewModel, Source={StaticResource Locator}}">

  <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
    <StackPanel Margin="{StaticResource PageMargin}">
    <!--TODO: Add MVVMLightBlankPageTitle.Text to String Resources-->
      <TextBlock
          x:Uid="MVVMLightBlankPageTitle"
          Style="{ThemeResource HeaderTextBlockStyle}"/>
    </StackPanel>
  </Grid>
</Page>