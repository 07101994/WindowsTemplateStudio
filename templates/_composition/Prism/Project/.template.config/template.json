{
    "author": "Brian Lagunas",
    "classifications": [
      "Universal"
    ],
    "name": "Prism.Project",
    "tags": {
      "language": "C#",
      "type": "item",
      "wts.type": "composition",    
      "wts.version": "1.0.0",    
      "wts.licenses": "[Prism](https://github.com/PrismLibrary/Prism/blob/master/LICENSE)",
      "wts.compositionFilter": "$framework == Prism & wts.type == project"
    },
    "sourceName": "wts.ItemName",
    "preferNameDirectory": true,
    "PrimaryOutputs": [
      { "path": ".\\App.xaml" }
    ],
    "symbols": {
      "wts.rootNamespace": {
        "type": "parameter",
        "replaces": "Param_RootNamespace"
      },
      "wts.itemNamespace": {
        "type": "parameter",
        "replaces": "Param_ItemNamespace"
      }
    }
  }
  